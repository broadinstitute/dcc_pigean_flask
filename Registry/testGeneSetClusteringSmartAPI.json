        "responses": {
          "200": {
            "description": "Gene set clustering results",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "clusters": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "cluster_id": {
                            "type": "string"
                          },
                          "genes": {
                            "type": "array",
                            "items": {
                              "type": "string"
                            }
                          },
                          "enrichment_score": {
                            "type": "number"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
